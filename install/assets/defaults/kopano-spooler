#!/command/with-contenv bash

SPOOLER_ALLOW_DELEGATE_MEETING_REQUEST=${SPOOLER_ALLOW_DELEGATE_MEETING_REQUEST:-"TRUE"}
SPOOLER_ALLOW_REDIRECT_SPOOFING=${SPOOLER_ALLOW_REDIRECT_SPOOFING:-"TRUE"}
SPOOLER_ALLOW_SEND_TO_EVERYONE=${SPOOLER_ALLOW_SEND_TO_EVERYONE:-"TRUE"}
SPOOLER_ALWAYS_SEND_DELEGATES=${SPOOLER_ALWAYS_SEND_DELEGATES:-"FALSE"}
SPOOLER_ENABLE_DSN=${SPOOLER_ENABLE_DSN:-"TRUE"}
SPOOLER_EXPAND_GROUPS=${SPOOLER_EXPAND_GROUPS:-"FALSE"}
SPOOLER_LOG_RAW_MESSAGE_STAGE1=${SPOOLER_LOG_RAW_MESSAGE_STAGE1:-"FALSE"}
SPOOLER_MAX_THREADS=${SPOOLER_MAX_THREADS:-5}
SPOOLER_PATH_PLUGIN=${SPOOLER_PATH_PLUGIN:-"/data/spooler/plugins/"}
SPOOLER_PATH_RAW_MESSAGES=${SPOOLER_PATH_RAW_MESSAGES:-"/data/spooler/raw_messages/"}
SPOOLER_PLUGIN_ENABLED=${SPOOLER_PLUGIN_ENABLED:-"FALSE"}
SPOOLER_SMTP_HOST=${SPOOLER_SMTP_HOST:-localhost}
SPOOLER_SMTP_PORT=${SPOOLER_SMTP_PORT:-25}
SPOOLER_SSL_CERT_FILE=${SPOOLER_SSL_CERT_FILE:-"/certs/core/spooler.crt"}
SPOOLER_SSL_KEY_FILE=${SPOOLER_SSL_KEY_FILE:-"/certs/core/spooler.pem"}